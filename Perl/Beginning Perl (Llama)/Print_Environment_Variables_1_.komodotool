  "name": "Print Environment Variables [1]", 
  "value": [
    "use strict;", 
    "use integer;", 
    "", 
    "while(my ($k,$v) = each(%ENV)) {", 
    "    undef($v) unless $v;", 
    "    printf \"%28s  %50s\\n\", $k, ($v // \"<undefined value>\");", 
    "}!@#_currentPos!@#_anchor"
  ], 
