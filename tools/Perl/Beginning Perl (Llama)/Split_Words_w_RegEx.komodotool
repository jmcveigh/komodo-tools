  "name": "Split Words w/ RegEx", 
  "value": [
    "while(<>) {", 
    "  if (/^.*[ +\\t]$/) {", 
    "    chomp;", 
    "    print '\\'';", 
    "    print;", 
    "    print \"'\\n\";", 
    "  }", 
    "}!@#_currentPos!@#_anchor"
